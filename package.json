{
  "name": "loan-prediction-dashboard",
  "version": "1.0.0",
  "description": "A complete loan prediction dashboard MVP with ML model and React frontend",
  "main": "server/index.js",
  "scripts": {
    "setup": "npm install && cd client && npm install",
    "start:server": "node server/index.js",
    "start:client": "cd client && npm start",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "express": "^4.21.2",
    "ml-cart": "^2.1.1",
    "ml-logistic-regression": "^2.0.0",
    "ml-matrix": "^6.12.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "loan",
    "prediction",
    "dashboard",
    "ml",
    "react"
  ],
  "author": "AI Assistant",
  "license": "MIT"
}
